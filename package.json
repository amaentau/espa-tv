{
  "name": "veo-dongle",
  "version": "1.0.0",
  "description": "Multi-platform system for Veo stream playback and control",
  "main": "index.js",
  "scripts": {
    "install:all": "npm run install:raspberry && npm run install:bbs",
    "install:raspberry": "cd raspberry-pi && npm install",
    "install:bbs": "cd bbs && npm install",
    "start:raspberry": "cd raspberry-pi && npm start",
    "start:bbs": "cd bbs && npm start",
    "play": "cd raspberry-pi && npm run play",
    "help": "cd raspberry-pi && npm run help",
    "dev:raspberry": "cd raspberry-pi && npm run dev"
  },
  "workspaces": [
    "raspberry-pi",
    "bbs"
  ],
  "keywords": [
    "veo",
    "stream",
    "raspberry-pi",
    "cloud",
    "bbs"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {}
}
